        </select> <br><br>
        <input type="submit" value="Перейти" />
    </form>

    <div style="font-size:24px">Частное представление</div><br>
    <form method="post" action="/look">
        <label>type_exp_id</label><br>
        <select name="type_id" class="field">
            <% for (var i = 0; i < type_ids.length; i++) { %>
                <option value=<%= type_ids[i].id %>> id:<%= type_ids[i].id %>; <%= type_ids[i].name %> </option>
            <% } %>
        </select> <br><br>
        <label>Показать все ID</label><br>
        <select name="show_id">
            <option value=0 selected>Нет</option>
            <option value=1>Да</option>
        </select> <br><br>
        <input type="submit" value="Перейти" />
    </form>

    <div style="font-size:24px">Посмотреть все типы экспериментов с ID</div><br>
    <form method="post" action="/type_ids">
        <input type="submit" value="Перейти" />
    </form>

    <div style="font-size:24px">Скачать файлы, соответствующие запуску</div><br>
    <form action="/download" method="post">
        <label>ID запуска</label><br>
        <select name="launch_id" class="field">
            <% for (var i = 0; i < launch_ids.length; i++) { %>
                <option value=<%= launch_ids[i].id %>> id:<%= launch_ids[i].id %> </option>
            <% } %>
        </select> <br><br>
        <input type="submit" value="Перейти" />
    </form>
</body>