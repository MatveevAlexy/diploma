    </form>

    <div style="font-size:24px">Удалить эксперимент</div><br>
    <form action="/del_exp" method="post">
        <label>ID эксперимента</label><br>
        <select name="exp_id" class="field">
            <% for (var i = 0; i < exp_ids.length; i++) { %>
                <option value=<%= exp_ids[i].id %>> id:<%= exp_ids[i].id %>; <%= exp_ids[i].exp_name %> </option>
            <% } %>
        </select> <br><br>
        <input type="submit" value="Перейти" />
    </form>

    <div style="font-size:24px">Новый запуск</div><br>
    <form action="/new_launch" method="post">
        <label>ID соответствующего эксперимента</label><br>
        <select name="exp_id" class="field">
            <% for (var i = 0; i < exp_ids.length; i++) { %>
                <option value=<%= exp_ids[i].id %>> id:<%= exp_ids[i].id %>; <%= exp_ids[i].exp_name %> </option>
            <% } %>
        </select> <br><br>
        <input type="submit" value="Перейти" />
    </form>

    <div style="font-size:24px">Изменить запуск</div><br>
    <form action="/upd_launch" method="post">
        <label>ID запуска</label><br>
        <select name="launch_id" class="field">
            <% for (var i = 0; i < launch_ids.length; i++) { %>
                <option value=<%= launch_ids[i].id %>> id:<%= launch_ids[i].id %> </option>
            <% } %>
        </select> <br><br>
        <label>Изменить переменные параметры</label><br>
        <select name="ch_vcond">
            <option value=0 selected>Нет</option>
            <option value=1>Да</option>
        </select> <br><br>
        <label>Изменить результаты</label><br>
        <select name="ch_result">
